@model IEnumerable<Categories>

@{

}

 <div class="container">
    <div class="row mt-5">
      
            @foreach (var items in Model) {
                <div class="col">
                    <a asp-action=@items.Url>
                    <div class="card">
                        <div class="card-body mt-5">
                            <div class="row">
                                <div class="col">
                                    @items.Name 
                                </div>

                                <div class="col">
                                    @items.Icon
                                </div>
                            </div>
                        </div>
                    </div>
                    </a>
                </div>
                
             }
    </div>
 </div>
